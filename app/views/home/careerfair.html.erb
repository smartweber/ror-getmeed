<section class="promo section section-on-bg" data-sr='enter bottom and scale up 20% over 1s and reset'>
  <div class="container text-center">
    <div class="any-center">
      <img src="https://res.cloudinary.com/resume/image/upload/c_scale,w_120/v1427155934/crescent_green_rt6rit.png" style="padding: 20px;">
    </div>
    <h2 class="title" style="line-height: 1.1;">Meed Fair</h2>

    <p class="intro text-center"><span style="font-weight: bold; font-size:20px">Deadline
      for applications â€” June 5th</span>
    </p>

    <%= form_tag({:controller => 'users', :action => 'verify'}, :class => 'signup-form navbar-form', :html => {:multipart => true}) do %>
        <%= hidden_field_tag :source, 'meed_fair' %>
        <div class="form-group">
          <%= text_field_tag :email, nil, :required => '', :class => 'form-control', :id => 'sign_up_tb', :style => 'height:50px;', :placeholder => 'Enter school email (ex: sam@usc.edu)' %>
        </div>
        <%= button_tag(type: 'submit', class: 'btn btn-cta btn-cta-primary', :id => 'sign_up_tb', :data => {:disable_with => 'Verifying.. '}) do %>
            Get In
        <% end %>
        <p>
        </p>

        <div class="col-md-2 col-md-offset-4">
          <% if !alert.blank? %>
              <p class="label label-important">
                <%= alert %>
              </p>
          <% end %>
        </div>
    <% end -%>
  </div>
</section>
